{
  "name": "newman-env",
  "version": "1.0.2",
  "description": "Command-line utility for update Postman Environment",
  "homepage": "https://github.com/GeminiWind/newman-env",
  "bugs": "https://github.com/GeminiWind/newman-env/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:GeminiWind/newman-env.git"
  },
  "keywords": [
    "newman-env",
    "newman",
    "postman"
  ],
  "bin": {
    "newman-env": "./dist/bin/newman-env.js"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
